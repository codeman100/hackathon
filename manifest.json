{
    "manifest_version": 2,
    "name": "Your Extension Name",
    "version": "1.0",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "https://example.com/loader.css"
    ],
    "web_accessible_resources": [
      "loader.css" 
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  